{
  "name": "toolcase-api",
  "main": "workers/worker-api-exchange-rates.js",
  "compatibility_date": "2025-01-13",
  "compatibility_flags": [
    "nodejs_compat_v2"
  ],
  "account_id": "1ff43f0d4c3ad3bd98ce5ab767546a68",
  "alias": {
    "critters": "./workers/shims/empty.js",
    "@opentelemetry/api": "./workers/shims/empty.js"
  },
  "env": {
    "production": {
      "name": "toolcase-api-production",
      "routes": [
        {
          "pattern": "toolcase.cc/api/exchange-rates*",
          "zone_name": "toolcase.cc"
        },
        {
          "pattern": "www.toolcase.cc/api/exchange-rates*",
          "zone_name": "toolcase.cc"
        }
      ],
      "vars": {
        "ENVIRONMENT": "production"
      }
    },
    "preview": {
      "name": "toolcase-api-preview",
      "vars": {
        "ENVIRONMENT": "preview"
      }
    }
  },
  "base_dir": "."
}
